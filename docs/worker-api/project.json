{
  "name": "worker-api",
  "root": "apps/worker-api",
  "sourceRoot": "apps/worker-api/src",
  "projectType": "application",
  "targets": {
      "dev": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npm run dev -- --tenant=mrrainbowsmoke"
        }
      },
    "dev-tenant": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev --"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm test"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run typecheck"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/generate-tenant-index.mjs && npx wrangler deploy"
      }
    },
    "postman-perf": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx newman run postman-collection.json -e postman-env.staging.json --reporters cli,json --reporter-json-export docs/m4-results/newman-performance.json",
        "cwd": "."
      }
    }
  }
}
