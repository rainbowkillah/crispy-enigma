{
  "name": "tenant-alpha",
  "root": "tenants/alpha",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev -- --tenant=alpha"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/generate-tenant-index.mjs && npx wrangler deploy --config tenants/alpha/wrangler.jsonc"
      }
    }
  }
}
